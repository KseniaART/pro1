<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fon"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="220sp"
        android:layout_height="150sp"
        android:layout_marginStart="5sp"
        android:layout_marginTop="130sp"
        android:src="@drawable/ndva"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="170sp"
        android:layout_height="150sp"
        android:layout_marginTop="128sp"
        android:layout_marginEnd="5sp"
        android:src="@drawable/raf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30sp"
        android:text="@string/scorendva"
        android:textColor="@color/waf"
        android:textSize="@dimen/chet"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30sp"
        android:layout_marginEnd="50sp"
        android:text="@string/scoreraf"
        android:textColor="@color/waf"
        android:textSize="@dimen/chet"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40sp"
        android:text="@string/who"
        android:textColor="@color/waf"
        android:textSize="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="200sp"
        android:text="@string/colon"
        android:textColor="@color/waf"
        android:textSize="@dimen/toch"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="@dimen/widhbuttonone"
        android:layout_height="@dimen/heightbuttonone"
        android:layout_marginTop="100sp"
        android:backgroundTint="@color/buttoncolor"
        android:text="@string/raf"
        android:textSize="@dimen/girl"
        android:textColor="@color/gaf"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/button2"
        android:layout_width="@dimen/widhbuttonone"
        android:layout_height="@dimen/heightbuttonone"
        android:layout_marginTop="99sp"
        android:layout_marginEnd="50sp"
        android:backgroundTint="@color/buttoncolor"
        android:text="@string/ndva"
        android:textSize="@dimen/girl"
        android:textColor="@color/gaf"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:layout_width="@dimen/widthbuttontwo"
        android:layout_height="@dimen/eightbuttonone"
        android:layout_marginTop="@dimen/niq"
        android:backgroundTint="@color/buttoncolor"
        android:text="@string/reset"
        android:textSize="@dimen/res"
        android:textColor="@color/gaf"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>